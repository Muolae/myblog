le.log(state.value)\r\n        }\r\n        function toaddr(url){\r\n            window.location.href =url\r\n        }\r\n        function newnote(){\r\n            router.push({\r\n                path:'/newnote',\r\n\r\n            })\r\n        }\r\n        return {\r\n            state,\r\n            changestate,\r\n            toaddr,\r\n            newnote\r\n        }\r\n\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n.nav{\r\n    background-color: rgb(247, 246, 246);\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n}\r\n.headpic{\r\n    margin-top: 20px;\r\n    height: 60px;\r\n    width: 60px;\r\n}\r\n.navbtn{\r\n    width: 80%;\r\n    height: 40px;\r\n    border-radius: 4px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    margin-top: 10px;\r\n}\r\n.navbtn:hover{\r\n    background-color: white;\r\n}\r\n.newnote{\r\n    width: 150px;\r\n    background-color: rgb(199, 129, 24);\r\n    height: 30px;\r\n    border-radius: 30px;\r\n    \r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    margin-top: 200px;\r\n    color: white;\r\n}\r\n.onclose{\r\n    width: 80%;\r\n    height: 40px;\r\n    border-radius: 4px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    background-color: white;\r\n    margin-top: 10px;\r\n    color: rgb(142, 142, 26);\r\n}\r\n.iconpic{\r\n    width: 20px;\r\n    height: 20px;\r\n    margin-right: 20px;\r\n}\r\n.num{\r\n   margin-left: 20px;\r\n    margin-top: 2px;\r\n    width: 30px;\r\n}\r\n.th3{\r\n    font-family: \"Sofia\";\r\n}\r\n.link{\r\n    display: flex;\r\n    width: 100%;\r\n    justify-content: center;\r\n    align-items: center;\r\n    position: absolute;\r\n    bottom: 20px;\r\n}\r\n.linkpic{\r\n    width: 30px;\r\n    height: 30px;\r\n    margin-left: 20px;\r\n    opacity: 0.7;\r\n}\r\n.linkpic:hover{\r\n    opacity: 1.0;\r\n}\r\nspan{\r\n    width: 40px;\r\n    word-wrap: normal;\r\n}\r\n</style>"],"mappings":";AAoCA,SAAQA,GAAG,QAAO,KAAI;AACtB,SAAQC,SAAS,QAAO,YAAW;AACnC,eAAe;EACXC,KAAKA,CAAA,EAAE;IACH,IAAIC,KAAK,GAACH,GAAG,CAAC,CAAC;IACf,MAAMI,MAAM,GAACH,SAAS,CAAC;IACvB,SAASI,WAAWA,CAACC,KAAK,EAAC;MACvBH,KAAK,CAACG,KAAK,GAACA,KAAI;MAChB,IAAGA,KAAK,IAAE,CAAC,EAAC;QACPF,MAAM,CAACG,OAAO,CAAC;UAChBC,IAAI,EAAC,UAAU;UACfC,KAAK,EAAC;YACFC,IAAI,EAAC;UACT;QACA,CAAC;MACL;MACA,IAAGJ,KAAK,IAAE,CAAC,EAAC;QACPF,MAAM,CAACG,OAAO,CAAC;UAChBC,IAAI,EAAC,UAAU;UACfC,KAAK,EAAC;YACFC,IAAI,EAAC;UACT;QACA,CAAC;MACL;MACA,IAAGJ,KAAK,IAAE,CAAC,EAAC;QACPF,MAAM,CAACG,OAAO,CAAC;UAChBC,IAAI,EAAC,QAAQ;UACbC,KAAK,EAAC;YACFC,IAAI,EAAC;UACT;QACA,CAAC;MACL;;MAEA;IACJ;IACA,SAASC,MAAMA,CAACC,GAAG,EAAC;MAChBC,MAAM,CAACC,QAAQ,CAACC,IAAG,GAAGH,GAAE;IAC5B;IACA,SAASI,OAAOA,CAAA,EAAE;MACdZ,MAAM,CAACa,IAAI,CAAC;QACRT,IAAI,EAAC;MAET,CAAC;IACL;IACA,OAAO;MACHL,KAAK;MACLE,WAAW;MACXM,MAAM;MACNK;IACJ;EAEJ;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}